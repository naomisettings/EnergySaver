<?xml version="1.0" encoding="utf-8"?>


<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/degradat"
        tools:context=".medalles.Medalla3Fragment">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imgViewLogo"
                android:layout_width="277dp"
                android:layout_height="51dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="35dp"
                android:layout_marginEnd="16dp"
                android:contentDescription="@string/medallaEstalviadorGran"
                android:src="@drawable/energy2"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/imgViewMedallaGran"
                android:layout_width="223dp"
                android:layout_height="230dp"
                android:layout_marginTop="35dp"
                android:contentDescription="@string/logoESmedalles"
                android:src="@drawable/medallaoriginal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imgViewLogo" />

            <TextView
                android:id="@+id/multiAutoCompleteTextView"
                style="@style/ColorsBasicsAplicacio"
                android:layout_width="308dp"
                android:layout_height="168dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="35dp"
                android:layout_marginEnd="16dp"
                android:text="@string/multiLineTxtViewMedalla3"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imgViewMedallaGran"
                tools:ignore="LabelFor" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </FrameLayout>
</layout>