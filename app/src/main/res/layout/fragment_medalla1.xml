<?xml version="1.0" encoding="utf-8"?>

<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

<FrameLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".medalles.Medalla1Fragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/imgViewLogo"
            android:layout_width="277dp"
            android:layout_height="51dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="35dp"
            android:layout_marginEnd="16dp"
            android:src="@drawable/energy2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:contentDescription="@string/medallaEstalviadorGran" />

        <ImageView
            android:id="@+id/imgViewMedallaGran"
            android:layout_width="223dp"
            android:layout_height="230dp"
            android:layout_marginTop="35dp"
            android:contentDescription="@string/logoESmedalles"
            android:src="@drawable/medallaoriginal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imgViewLogo" />

        <MultiAutoCompleteTextView
            android:id="@+id/multiAutoCompleteTextView"
            android:layout_width="292dp"
            android:layout_height="91dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="35dp"
            android:layout_marginEnd="16dp"
            android:text="@string/multiLineTxtViewMedalla1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            style="@style/ColorsBasicsAplicacio"
            tools:ignore="LabelFor"
            app:layout_constraintTop_toBottomOf="@+id/imgViewMedallaGran" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>
</layout>